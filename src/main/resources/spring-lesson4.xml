<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd" >

<bean id="triangle" class="beanFactory.lesson4.Triangle" >
    <property name="pointA" ref="pointA" />
    <property name="pointB" ref="pointB" />
    <property name="pointC" ref="pointC" />
</bean>

    <bean id="pointA" class="beanFactory.lesson4.Point" >
        <property name="x" value="0" />
        <property name="y" value="0" />
    </bean>
    <bean id="pointB" class="beanFactory.lesson4.Point" >
        <property name="x" value="10" />
        <property name="y" value="20" />
    </bean>
    <bean id="pointC" class="beanFactory.lesson4.Point" >
        <property name="x" value="35" />
        <property name="y" value="55" />
    </bean>


</beans>